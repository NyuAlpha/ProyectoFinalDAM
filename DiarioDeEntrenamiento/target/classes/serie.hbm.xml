<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "POJOs.Serie" table = "series">
      
      <meta attribute = "class-description">
         This class contains the employee detail. 
      </meta>

      <composite-id>
        <key-property name = "numSerie" type = "int" column = "num_Serie" />
        <key-property name = "idEjercicio" type = "int" column = "id_ejercicio" />
      </composite-id>
      
      <property name = "peso" column = "peso" type = "float"/>
      <property name = "repeticiones" column = "repeticiones" type = "int"/>
      <property name = "extra" column = "extra" type = "string"/>
      
   </class>
</hibernate-mapping>

